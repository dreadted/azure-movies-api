  "dependencies": {
    "@types/connect-slashes": "0.0.32",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/lodash": "^4.14.150",
    "@types/mssql": "^6.0.0",
    "common-tags": "^1.8.0",
    "concurrently": "^5.2.0",
    "connect-slashes": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "mssql": "^6.2.0",
